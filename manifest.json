{
    "manifest_version": 2,
    "name": "Qrcoder",
    "version": "1.0",
    "description": "Simple QR Code Generator",
    "homepage_url": "https://github.com/edwin0cheng/qrcoder",
    "icons": {
        "48": "icons/icon-48.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/icon-48.png",
        "default_title": "QRCoder",
        "default_popup": "popup/qrcoder.html"
    },
    "web_accessible_resources": []
}